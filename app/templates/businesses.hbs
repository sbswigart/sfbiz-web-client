<div class="table-resonsive">
  <table class="table">
    <thead>
      <tr>
        <th></th>
        <th>Name</th>
        <th>Address</th>
        <th>Start Date</th>
        <th>End Date</th>
      </tr>
    </thead>
    <tbody>
      {{#each model as |biz|}}
      <tr>
        <td><li class="fa fa-heart-o"></li></td>
        <td>{{biz.name}}</td>
        <td>{{biz.address}}</td>
        <td>{{biz.startDate}}</td>
        <td>{{biz.endDate}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>
